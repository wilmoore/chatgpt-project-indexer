{
  "name": "chatgpt-project-indexer",
  "version": "1.7.0",
  "description": "Enumerate all ChatGPT Projects via browser automation",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "chatgpt-indexer": "./dist/index.js"
  },
  "scripts": {
    "dev": "npx supabase start && npm run auth:login && tsx --watch src/index.ts run --watch --interval 1m",
    "build": "tsc",
    "start": "node dist/index.js",
    "watch": "node dist/index.js run --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "auth:login": "tsx src/index.ts auth login",
    "auth:status": "tsx src/index.ts auth status",
    "auth:export": "tsx src/index.ts auth export"
  },
  "keywords": [
    "chatgpt",
    "projects",
    "indexer",
    "automation"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.90.1",
    "commander": "^14.0.2",
    "playwright": "^1.57.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
